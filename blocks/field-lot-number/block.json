{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "aucteeno/field-lot-number",
	"title": "Aucteeno Lot Number",
	"category": "aucteeno",
	"parent": [ "aucteeno/card" ],
	"icon": "editor-ol",
	"description": "Display the lot number for items.",
	"keywords": [ "aucteeno", "lot", "number" ],
	"textdomain": "aucteeno",
	"usesContext": [ "aucteeno/item", "aucteeno/itemType" ],
	"supports": { "html": false, "spacing": { "margin": true, "padding": true } },
	"attributes": {
		"showLabel": { "type": "boolean", "default": true },
		"prefix": { "type": "string", "default": "Lot #" }
	},
	"editorScript": "file:../../dist/blocks/field-lot-number/editor.js",
	"style": "file:../../dist/blocks/field-lot-number/style-editor.css",
	"render": "file:./render.php"
}
