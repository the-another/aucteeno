{
  "name": "theanother/aucteeno",
  "description": "Custom WooCommerce plugin for auction and item management",
  "type": "wordpress-plugin",
  "license": "GPL-2.0-or-later",
  "version": "1.0.6",
  "author": {
    "name": "The Another",
    "email": "hello@theanother.org",
    "url": "https://theanother.org"
  },
  "keywords": [
    "wordpress",
    "plugin",
    "woocommerce",
    "auction",
    "item",
    "management"
  ],
  "homepage": "https://theanother.org/plugin/aucteeno/",
  "support": {
    "issues": "https://github.com/theanother/aucteeno/issues",
    "source": "https://github.com/theanother/aucteeno"
  },
  "require": {
    "php": ">=8.3"
  },
  "require-dev": {
    "automattic/vipwpcs": "^3.0",
    "brain/monkey": "^2.6",
    "coenjacobs/mozart": "^0.7.1",
    "composer/composer": "^2.7",
    "johnpbloch/wordpress-core": "^6.9",
    "mockery/mockery": "^1.6",
    "php-stubs/woocommerce-stubs": "^10.4",
    "php-stubs/wordpress-stubs": "^6.9",
    "phpunit/phpunit": "^11.0",
    "squizlabs/php_codesniffer": "^3.9",
    "wp-coding-standards/wpcs": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "TheAnother\\Plugin\\Aucteeno\\Dependencies\\": "dependencies/"
    },
    "classmap": [
      "includes/"
    ],
    "files": []
  },
  "autoload-dev": {
    "psr-4": {
      "TheAnother\\Plugin\\Aucteeno\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    },
    "sort-packages": true
  },
  "extra": {
    "mozart": {
      "dep_namespace": "TheAnother\\Plugin\\Aucteeno\\Dependencies\\",
      "dep_directory": "/dependencies/",
      "classmap_directory": "/includes/classes/",
      "classmap_prefix": "TheAnother_Plugin_Aucteeno_",
      "packages": []
    }
  },
  "scripts": {
    "phpcs": "phpcs",
    "phpcbf": "phpcbf",
    "test": "phpunit",
    "mozart": "mozart compose && node scripts/move-production-vendor.js",
    "build": [
      "composer install -q --no-suggest",
      "@composer mozart",
      "node scripts/remove-vendor.js",
      "@composer dump-autoload"
    ]
  }
}
